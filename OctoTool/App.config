<?xml version="1.0" encoding="utf-8"?>

<configuration>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <appSettings>
        <add key="OctoBaseUrl" value="${Your Octopus Deploy Server}"/>
        <add key="OctoAPIKey" value="${Your API Key}"/>
        <add key="TimeOutAfterMinutes" value="5"/>
        <add key="MachineTimeoutAfterMinutes" value="5"/>
        <add key="ReleasesSelectionRange" value="15"/>
        
        <add key="RestartScript" value="Restart-Computer -Force"/>
    </appSettings>
</configuration>